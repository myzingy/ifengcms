html,body{padding:0;margin:0;background:#f1f5f6;}
/*左侧导航样式开始*/
/*首页样式开始*/
header{background:#fff; color:#333; text-align:center;line-height:40px;height:40px;position:relative;}
header .l_tbn{position:absolute; left:0; top:0; width:100%; height:40px;}
header .l_tbn a{background-color:#f8faf9; display:block; width:40px; height:40px; overflow:hidden; color:#25b5c2; text-decoration:none;}
header .l_tbn a:hover{color:#00949b;}
.moretip{display:block; font-size:14px; line-height:40px; width:20px; height:40px; color:#999; position:absolute; top:0; right:0;}



.rightbtn{padding-right:10px; font-size:14px;}

/*左侧导航样式结束*/

/*滑动导航部分样式开始*/

.promptu-menu{color:#fff; width:100%; color:#000; }
.promptumenu_nav{z-index: 5; position: absolute; bottom:5px; left: 50%; margin-right: -50%; color:#fff;}
.promptumenu_window{cursor: url(hand.ico), move; background:#fff; position: relative;}
.promptumenu_nav a{
	cursor: pointer;
	width: 20px;
	height: 20px;
	background-color:#333;
	text-indent: -9999px;
	outline: none;
	display: block;
	float: left;
	position: relative;
	left: -50%;
	margin: 0 2px;
}
.promptumenu_nav a.active{background: url(pagelink.png) 0 0 no-repeat;}

.promptu-menu li{ width:25%; float: left;}
.promptu-menu li.active{border-bottom:2px solid #25b5c2; color:#25b5c2;}
.promptu-menu li h3{font:16px/38px "微软雅黑";}
.promptu-menu li.active h3{font:18px/38px "微软雅黑";}
.navbox{padding-left:0px;}

/*滑动导航部分样式结束*/

/*幻灯片样式开始*/
.lunbo{position:relative;}
.lunbo ul{ margin:0px; padding:0px;}
.lunbo #nav{position:absolute; width:100%; height:20px; line-height:20px;  bottom:0px; right:0px;}
.lunbo #nav ul{float:right; padding-right:10px; padding-top:8px;}
.lunbo #nav ul li{float:left; background:#999; border-radius:50%; width:6px; height:6px; margin-left:6px; opacity:0.5;}
.lunbo #nav ul li.active{ background:#666;opacity:1;}
/*增加的轮播部分*/
.lunbo ul li a{color:#333;}
.lunbo ul li a h3{padding-left:10px; font:14px/26px "微软雅黑";}
.lunbo .banner img{width:100%;}
.lunbo .banner ul li{ float:left;}
.lunbo .banner ul li .img{height:200px; overflow:hidden;}
.lunbo .banner ul li .label i{font-size:26px; color:#25b5c2; line-height:26px; margin-right:3px;}
/*幻灯片样式结束*/


/*新闻列表样式*/
.news{padding:10px 0;}
.news li{padding:10px 2%; border-top:1px solid #fff; border-bottom:1px solid #d9dadc;}
.news li:first-child{border-top:none;}
.news li a{color:#7d7d7d; font:14px/18px "微软雅黑";}
.news li a h3{color:#333; font:16px/1.3 "微软雅黑"; height:40px; overflow:hidden;}
.news li a h3.row01{color:#333; font:16px/1.5 "微软雅黑"; height:24px; overflow:hidden;}
.news li .img{width:30%; padding-right:2%; max-height:62px; overflow:hidden;}
.news li .mag{width:68%; position:relative; height:62px; overflow:hidden;}
.news li .mag p.text{height:36px; overflow:hidden;}
.news li .mag .thread{/*position:absolute; right:0; bottom:0; background-color:#f1f5f6;*/}
.news li .mag .thread i{color:#25b5c2; padding-left:4px;}
.news li .mag .label{margin-left:5px; border-radius:3px; padding:0 3px; border:1px solid #25b5c2; color:#25b5c2; font:10px/10px "微软雅黑";}
.imgs div{margin:4px 1%; margin-bottom:0; width:31.33%; max-height:80px; overflow:hidden;}
/*新闻列表样式结束*/

/*互动栏目样式开始*/
.interaction{padding:10px 0;}
.interaction li{margin-bottom:10px; padding:10px 0; padding-bottom:0; background-color:#fff; text-align:left;}
.interaction li a{color:#7d7d7d; display:block; font:14px/18px "微软雅黑"; position:relative; padding-left:50px; padding-right:10px;}
.interaction li .mag h3{color:#333; font:14px/20px "微软雅黑";  height:20px; margin-bottom:4px; overflow:hidden; }
.interaction li .mag p.text{height:36px; overflow:hidden;}
.interaction li .userimg{position:absolute; left:10px; top:0; border-radius:50%; width:30px; height:30px; overflow:hidden;}
.interaction li .userimg img{border-radius:50%; width:30px; height:30px; overflow:hidden;}
.interaction li .name p{width:50%; text-align:left; font:bold 14px/26px "微软雅黑"; color:#000;}
.interaction li .name p.time{font:12px/26px "微软雅黑"; color:#666; text-align:right;}
.interaction li .mag{padding:10px 0;}
.interaction .button-groups div{width:33.33%; border-top:1px solid #efefef; border-top:1px solid #efefef;}
.interaction .button-groups div button{width:100%; cursor:pointer; border:none; background-color:#fff; font:14px/30px "微软雅黑"; color:#a6aba9; }
.interaction .button-groups div button i{font-size:20px;}
.interaction .button-groups div button.avtived i{color:#25b5c2;}

/*互动栏目样式结束*/

/*底部回复开始*/
.reply{display:none; position:fixed; bottom:0px; left:0; z-index:10; width:96%; background-color:#E2E2E2; padding:5px 2%;}
.reply .replybox{padding-right:40px; position:relative;}
.reply .replybox .replybg{width:100%; position:relative;}
.reply .replybox .tip{position:absolute; top:0; left:0;  color:#808080;}
.reply .replybox .tip i{font-size:24px; line-height:24px;}
.reply .replybox .tip i span{font:12px/24px "微软雅黑";}
.reply input{width:96%; padding:3px 2%; font:12px/24px "微软雅黑"; border:0; border-radius:3px; background-color:#f1f5f6;}
.reply .selease{position:absolute; right:8px; top:6px; width:30px; height:30px; border:none; background-color:transparent; color:#808080; cursor:pointer;}
.reply .selease i{font-size:24px; line-height:24px;}
.focus .selease{color:#25b5c2;}
.focus input{border:1px solid #25b5c2; padding:2px 2%;}
.focus .tip{display:none;}
.show{display:block;}
/*底部回复开始*/
/*加载更多按钮*/
.more{padding:10px 0; position:relative;}
.more .buttonbox{width:100px; margin:0 auto;}
.more .morebtn{width:100px; cursor:pointer; font:14px/30px "微软雅黑"; border:0; background-color:#f1f5f6; color:#25b5c2;}
.more .line{position:absolute; left:0; top:24px; z-index:-1;  width:100%; height:0; border-top:1px solid #fff; border-bottom:1px solid #d9dadc;}

/*首页样式结束*/

