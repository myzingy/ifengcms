@charset "utf-8";
/* CSS Document */
html,body,div,span,h1,h2,h3,h4,h5,h6,p,pre,a,code,em,img,small,strong,sub,sup,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label{margin:0;padding:0;border:0;outline:0; font-size: 14px; line-height: 18px;vertical-align:baseline;background:transparent}
a{color:#007bc4/*#424242*/; text-decoration:none;}
a:hover{text-decoration:underline}
ol,ul{list-style:none}
table{border-collapse:collapse;border-spacing:0}
body{height:100%; font:12px/18px "Microsoft Yahei", Tahoma, Helvetica, Arial, Verdana, "\5b8b\4f53", sans-serif; color:#51555C; background:#162934}
img{border:none}


#header{width:100%; height:0px; margin:0 auto; position:relative}
#logo{width:240px; height:90px;}
#logo h1{text-indent:-999em}
#logo h1 a{display:block; width:240px; height:90px}


#main{width:100%; height: 535px; margin:50px auto 0 auto; background:#fff; -moz-border-radius:12px;-khtml-border-radius: 12px;-webkit-border-radius: 12px; border-radius:12px;}
.top_title
{
    background: url("../../images/turncard.png") no-repeat 0 0;
    margin: 30px 20px;
    border-bottom: 1px solid #d3d3d3;
    height: 65px;
    background-size: 200px;
}

#footer{height:60px;}
#footer p{ padding:10px 2px; line-height:24px; text-align:center}
#footer p a:hover{color:#51555C}
#stat{display:none}


#prize li{float:left; width:49%; height:80px; line-height:80px; margin:1px; font-size:48px; text-align:center; color:#fff; cursor:pointer}
#viewother,#repeat{display:none;}
.google_ad{width:728px; height:90px; margin:50px auto}
.ad_76090,.ad_demo{width:760px; height:90px; margin:40px auto}
.demo_topad{position:absolute; top:15px; right:0px; width:640px; height:60px;}
.turncart_conten
{
    width: 90%;
    margin: 5px auto 0;
    height: 255px;
}
.turncart_conten p{height:40px}

.content {
    margin: 0px 0 15px 0;
}

.boxcontent {
    margin: 15px 15px 0;
    border-radius: 5px;
    padding: 2px;
    background-color: #FEF8B2;
    -webkit-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
    -moz-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
    -o-box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
    box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
}

.boxyellow {
    background-color: #FEF8B2;
}

.boxwhite {
    background-color: #ffffff;
}

.box {
    border-radius: 5px;
    font-size: 14px;
    border: 1px dashed rgba(0, 0, 0, 0.3);
}
.red {
    color: red;
}
.box .title-red {
    display: inline-block;
    margin: 5px 0 5px -3px;
    width: 145px;
    height: 25px;
    line-height: 28px;
    text-align: center;
    background: url("../../images/bg1.png") no-repeat 0 0;
    -webkit-background-size: auto 100%;
    color: #6d5939;
    font-size: 15px;
    font-weight: bold;
    /*-moz-text-shadow:0 1px 0 #8d0001;-webkit-text-shadow:0 1px 0 #8d0001;text-shadow:0 1px 0 #8d0001;
text-shadow:0px 1px 0 #8d0001;*/
}
.Detail {
    padding: 15px;
}
